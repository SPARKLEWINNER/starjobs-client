{
    "compilerOptions": {
        "target": "es6",
        "moduleResolution": "node",
        "module": "commonjs",
        "strict": true,
        "noEmit": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "baseUrl": "src",
    },
    "include": [
        "*",
        "src/app.service.js"
    ],
    "exclude": [
        "node_modules"
    ]
}